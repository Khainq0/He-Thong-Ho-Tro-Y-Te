@{
    ViewBag.Title = "Hệ chuyên gia";
}
@{
    Layout = "~/Views/Shared/HomeTemplate.cshtml";
}
<div class="clients_red">
    <div class="container">
        <div id="testimonial_slider" class="carousel slide" data-ride="carousel">
            <!-- Indicators -->
            <ul class="carousel-indicators">
            
            </ul>
            <!-- The slideshow -->
            <div class="carousel-inner">
                <div class="carousel-item active">

                    <div class="testomonial_section">
                        <div class="full center">
                        </div>
                        <div class="full testimonial_cont ">
                            <div class="row">
                                <div class="col-xl-3 col-lg-3 col-md-3 col-sm-12 pa_right">
                                    <div class="testomonial_img">
                                        <figure><img src="~/Content/images/unnamed.jpg" alt="#" /></figure>
                                        <i><img src="~/Content/images/test_con.png" alt="#" /></i>
                                    </div>
                                </div>
                                <div class="col-xl-9 col-lg-9 col-md-9 col-sm-12 pa_left">
                                    <div class="cross_inner">
                                        <h3>Hệ chuyên gia<br><strong class="ornage_color">Chẩn đoán</strong></h3>
                                        
                                        <h2>
                                            <label for="cars">Chọn bệnh cần tư vấn:</label>

                                            <select name="cars" id="cars">
                                                <option value="saab">Sốt xuất huyết</option>
                                                <option value="volvo">Tiêu chảy</option>
                                                <option value="mercedes">Viêm gan B</option>
                                                <option value="audi">Hẹp van tim</option>
                                                <option value="audi">Gout</option>
                                            </select>
                                            <a href="~/HeChuyenGia/Index">Go to HCG</a>
                                        </h2>
                                            
                                        
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>

                </div>
               
                </div>

               

        </div>
    </div>
</div>
